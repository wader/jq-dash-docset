{
    "name": "jq",
    "package": "jq",
    "index": "jq.html",
    "selectors": {
        "section > h2": {"type": "Section", "requiretext": "^[A-Z]"},
        "section > h3": {"type": "Section", "requiretext": "^[A-Z].*$|\\w.*[-/]"},

        "#Invoking > section > h3 code": {"type": "Expression", "requiretext": "^[^a-z]", "regexp": "\\n", "replace": " "},
        "#Basicfilters > section > h3 code": {"type": "Expression", "requiretext": "^[^a-z]", "regexp": "\\n", "replace": " "},
        "#TypesandValues > section > h3 code": {"type": "Expression", "requiretext": "^[^a-z]", "regexp": "\\n", "replace": " "},

        "#Builtinoperatorsandfunctions > section > h3 code": {"type": "Expression", "requiretext": "^[^a-z]", "regexp": "\\n", "replace": " "},
        "#Builtinoperatorsandfunctions > section > ul code": {"type": "Expression", "requiretext": "^[^a-z]", "regexp": "\\n", "replace": " "},

        "#ConditionalsandComparisons > section > h3 code": {"type": "Expression", "requiretext": "^[^a-z]", "regexp": "\\n", "replace": " "},
        "#RegularexpressionsPCRE > section > h3 code": {"type": "Expression", "requiretext": "^[^a-z]", "regexp": "\\n", "replace": " "},
        "#Advancedfeatures > section > h3 code": {"type": "Expression", "requiretext": "^[^a-z]", "regexp": "\\n", "replace": " "},
        "#Math > section > h3 code": {"type": "Expression", "requiretext": "^[^a-z]", "regexp": "\\n", "replace": " "},
        "#IO > section > h3 code": {"type": "Expression", "requiretext": "^[^a-z]", "regexp": "\\n", "replace": " "},
        "#Streaming > section > h3 code": {"type": "Expression", "requiretext": "^[^a-z]", "regexp": "\\n", "replace": " "},
        "#Assignment > section > h3 code": {"type": "Expression", "requiretext": "^[^a-z]", "regexp": "\\n", "replace": " "},

        "#Invoking > section > h3 code ": {"type": "Function", "requiretext": "^[a-z]\\w+", "regexp": "\\n", "replace": " "},
        "#Basicfilters > section > h3 code ": {"type": "Function", "requiretext": "^[a-z]\\w+", "regexp": "\\n", "replace": " "},
        "#TypesandValues > section > h3 code ": {"type": "Function", "requiretext": "^[a-z]\\w+", "regexp": "\\n", "replace": " "},

        "#Builtinoperatorsandfunctions section > h3 code ": {"type": "Function", "requiretext": "^[a-z]\\w+", "regexp": "\\n", "replace": " "},
        "#Builtinoperatorsandfunctions section > ul code ": {"type": "Function", "requiretext": "^[a-z]\\w+", "regexp": "\\n", "replace": " "},
        "#Builtinoperatorsandfunctions section > p code ": {"type": "Function", "requiretext": "^[a-z]\\w+", "regexp": "\\n", "replace": " "},

        "#ConditionalsandComparisons > section > h3 code ": {"type": "Function", "requiretext": "^[a-z]\\w+", "regexp": "\\n", "replace": " "},
        "#RegularexpressionsPCRE > section > h3 code ": {"type": "Function", "requiretext": "^[a-z]\\w+", "regexp": "\\n", "replace": " "},
        "#Advancedfeatures > section > h3 code ": {"type": "Function", "requiretext": "^[a-z]\\w+", "regexp": "\\n", "replace": " "},
        "#Math > section > h3 code ": {"type": "Function", "requiretext": "^[a-z]\\w+", "regexp": "\\n", "replace": " "},
        "#Math p code ": {"type": "Function", "requiretext": "^[a-z]\\w+", "regexp": "\\n", "replace": " "},

        "#IO > section > h3 code ": {"type": "Function", "requiretext": "^[a-z]\\w+", "regexp": "\\n", "replace": " "},
        "#IO > p code ": {"type": "Function", "requiretext": "^[a-z]\\w+", "regexp": "\\n", "replace": " "},

        "#Streaming > section > h3 code ": {"type": "Function", "requiretext": "^[a-z]\\w+", "regexp": "\\n", "replace": " "},
        "#Assignment > section > h3 code ": {"type": "Function", "requiretext": "^[a-z]\\w+", "regexp": "\\n", "replace": " "},

        "#Modules > section > h3 code": {"type": "Directive", "requiretext": "^\\w+"}
    },
    "ignore": ["sin()", "pow()"],
    "icon32x32": "",
    "allowJS": false,
    "ExternalURL": ""
}
