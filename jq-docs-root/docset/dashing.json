{
    "name": "jq",
    "package": "jq",
    "index": "jq.html",
    "selectors": {
        "section > h2": "Section",
        "#Advancedfeatures > section > h3": {"type": "Section"},
        "#Basicfilters > section > h3 > code": {"type": "Expression"},
        "#TypesandValues > section > h3 > code": {"type": "Expression"},
        "#Builtinoperatorsandfunctions h3 > code": {"type": "Function"},
        "#ConditionalsandComparisons > section > h3 > code": {"type": "Expression"},
        "#ConditionalsandComparisons > section#if-then-else > h3": {"type": "Function" },
        "#ConditionalsandComparisons > section#and\\/or\\/not > h3": {"type": "Function" },
        "#ConditionalsandComparisons > section#try-catch > h3": {"type": "Function" },
        "#RegularexpressionsPCRE > section > h3 > code": {"type": "Function"},
        "#Advancedfeatures > section > h3 > code": {
            "type": "Function",
            "requiretext": "^\\w+"
        },
        "#Math p > code": {"type": "Function"},
        "#IO p > code": {
            "type": "Function",
            "requiretext": "^\\w+"
        },
        "#Assignment > section > h3 > code": {"type": "Expression"},
        "#Modules > section > h3 > code": {"type": "Directive"}
    },
    "ignore": ["sin()", "pow()"],
    "icon32x32": "",
    "allowJS": false,
    "ExternalURL": ""
}
