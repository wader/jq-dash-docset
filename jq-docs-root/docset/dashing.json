{
    "name": "jq",
    "package": "jq",
    "index": "jq.html",
    "selectors": {
        "section > h2": {"type": "Section", "requiretext": "^[A-Z]"},
        "section > h3": {"type": "Section", "requiretext": "^[A-Z].*$|\\w.*[-/]"},

        "#invoking > section > h3 code": {"type": "Expression", "requiretext": "^[^a-z]", "regexp": "\\n", "replace": " "},
        "#basic-filters > section > h3 code": {"type": "Expression", "requiretext": "^[^a-z]", "regexp": "\\n", "replace": " "},
        "#type-sand-values > section > h3 code": {"type": "Expression", "requiretext": "^[^a-z]", "regexp": "\\n", "replace": " "},

        "#builtin-operators-and-functions > section > h3 code": {"type": "Expression", "requiretext": "^[^a-z]", "regexp": "\\n", "replace": " "},
        "#builtin-operators-and-functions > section > ul code": {"type": "Expression", "requiretext": "^[^a-z]", "regexp": "\\n", "replace": " "},

        "#conditionals-and-comparisons > section > h3 code": {"type": "Expression", "requiretext": "^[^a-z]", "regexp": "\\n", "replace": " "},
        "#regular-expressions > section > h3 code": {"type": "Expression", "requiretext": "^[^a-z]", "regexp": "\\n", "replace": " "},
        "#advanced-features > section > h3 code": {"type": "Expression", "requiretext": "^[^a-z]", "regexp": "\\n", "replace": " "},
        "#math > section > h3 code": {"type": "Expression", "requiretext": "^[^a-z]", "regexp": "\\n", "replace": " "},
        "#io > section > h3 code": {"type": "Expression", "requiretext": "^[^a-z]", "regexp": "\\n", "replace": " "},
        "#streaming > section > h3 code": {"type": "Expression", "requiretext": "^[^a-z]", "regexp": "\\n", "replace": " "},
        "#assignment > section > h3 code": {"type": "Expression", "requiretext": "^[^a-z]", "regexp": "\\n", "replace": " "},

        "#invoking > section > h3 > code ": {"type": "Function", "requiretext": "^[a-z]\\w+", "regexp": "\\n", "replace": " "},
        "#basic-filters > section > h3 > code ": {"type": "Function", "requiretext": "^[a-z]\\w+", "regexp": "\\n", "replace": " "},
        "#types-and-values > section > h3 > code ": {"type": "Function", "requiretext": "^[a-z]\\w+", "regexp": "\\n", "replace": " "},

        "#builtin-operators-and-functions section > h3 > code ": {"type": "Function", "requiretext": "^[a-z]\\w+", "regexp": "\\n", "replace": " "},

        "#conditionals-and-comparisons > section > h3 code ": {"type": "Function", "requiretext": "^[a-z]\\w+", "regexp": "\\n", "replace": " "},
        "#regular-expressions > section > h3 > code ": {"type": "Function", "requiretext": "^[a-z]\\w+", "regexp": "\\n", "replace": " "},
        "#advanced-features > section > h3 > code ": {"type": "Function", "requiretext": "^[a-z]\\w+", "regexp": "\\n", "replace": " "},
        "#math > section > h3 code ": {"type": "Function", "requiretext": "^[a-z]\\w+", "regexp": "\\n", "replace": " "},
        "#math p code ": {"type": "Function", "requiretext": "^[a-z]\\w+", "regexp": "\\n", "replace": " "},

        "#io > section > h3 code ": {"type": "Function", "requiretext": "^[a-z]\\w+", "regexp": "\\n", "replace": " "},
        "#io > p code ": {"type": "Function", "requiretext": "^[a-z]\\w+", "regexp": "\\n", "replace": " "},

        "#streaming > section > h3 code ": {"type": "Function", "requiretext": "^[a-z]\\w+", "regexp": "\\n", "replace": " "},
        "#assignment > section > h3 code ": {"type": "Function", "requiretext": "^[a-z]\\w+", "regexp": "\\n", "replace": " "},

        "#modules > section > h3 code": {"type": "Directive", "requiretext": "^\\w+"}
    },
    "ignore": ["sin()", "pow()"],
    "icon32x32": "icon@2x.png",
    "icon16x16": "icon.png",
    "allowJS": false,
    "ExternalURL": ""
}
